<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yangsun.me.cyclingguardian" >

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/my_app_theme" >
        <activity
            android:name=".LandingActivity"
            android:label="@string/title_activity_main_activity2" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".DisplayAccelerometerData"
            android:label="@string/title_activity_display_accelerometer_data" >
        </activity>
        <activity
            android:name=".GpsBasics"
            android:label="@string/title_activity_gps_basics"
            >
        </activity>
        <service
            android:name=".FetchAddressIntentService"
            android:exported="false"/>
    </application>

</manifest>
